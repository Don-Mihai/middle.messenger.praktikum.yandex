<div class='profile-module'>
    <div class='profile-module__avatar'>
        {{{avatar}}}
    </div>
    <h3 class='profile-module__title'>{{{userName}}}</h3>
    <div class='profile-module__inputs'>
        <div class='profile-module__input-wrapper'>
            {{{emailField}}}
            <span class='profile-module__input-value'>
                pochta@yandex.ru
            </span>
        </div>
        <div class='profile-module__input-wrapper'>
            {{{loginField}}}
            <span class='profile-module__input-value'>
                ivanivanov
            </span>
        </div>
        <div class='profile-module__input-wrapper'>
            {{{nameField}}}
            <span class='profile-module__input-value'>
                Иван
            </span>
        </div>
        <div class='profile-module__input-wrapper'>
            {{{lastnameField}}}
            <span class='profile-module__input-value'>
                Иванов
            </span>
        </div>
        <div class='profile-module__input-wrapper'>
            {{{chatNameField}}}
            <span class='profile-module__input-value'>
                Иван
            </span>
        </div>
        <div class='profile-module__input-wrapper'>
            {{{telField}}}
            <span class='profile-module__input-value'>
                +7 (909) 967 30 30
            </span>
        </div>
    </div>

    <div class='profile-module__actions'>
        {{#each actions}}
            {{{this}}}
        {{/each}}
    </div>
</div>